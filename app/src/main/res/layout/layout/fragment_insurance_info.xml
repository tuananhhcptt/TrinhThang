<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:textSize="12sp" android:textColor="#263d60" android:gravity="left" android:layout_width="0dp" android:layout_height="wrap_content" android:text="Bảo hiểm tai nạn cá nhân" android:layout_weight="1"/>
        <TextView android:textSize="12sp" android:textColor="#263d60" android:gravity="right" android:id="@+id/personal" android:layout_width="0dp" android:layout_height="wrap_content" android:text="nhiều tỷ đồng" android:layout_weight="1"/>
    </LinearLayout>
    <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp">
        <TextView android:textSize="12sp" android:textColor="#263d60" android:gravity="left" android:layout_width="0dp" android:layout_height="wrap_content" android:text="Bảo hiểm chi phí y tế" android:layout_weight="1"/>
        <TextView android:textSize="12sp" android:textColor="#263d60" android:gravity="right" android:id="@+id/medical" android:layout_width="0dp" android:layout_height="wrap_content" android:text="nhiều tỷ đồng" android:layout_weight="1"/>
    </LinearLayout>
    <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp">
        <TextView android:textSize="12sp" android:textColor="#263d60" android:gravity="left" android:layout_width="0dp" android:layout_height="wrap_content" android:text="Chi phí vân chuyển y tế cấp cứu" android:layout_weight="1"/>
        <TextView android:textSize="12sp" android:textColor="#263d60" android:gravity="right" android:id="@+id/transport" android:layout_width="0dp" android:layout_height="wrap_content" android:text="nhiều tỷ đồng" android:layout_weight="1"/>
    </LinearLayout>
    <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp">
        <TextView android:textSize="12sp" android:textColor="#263d60" android:gravity="left" android:layout_width="0dp" android:layout_height="wrap_content" android:text="Chi phí khác" android:layout_weight="1"/>
        <TextView android:textSize="12sp" android:textColor="#263d60" android:gravity="right" android:id="@+id/other" android:layout_width="0dp" android:layout_height="wrap_content" android:text="nhiều tỷ đồng" android:layout_weight="1"/>
    </LinearLayout>
    <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp">
        <TextView android:textSize="12sp" android:textColor="#263d60" android:gravity="left" android:layout_width="0dp" android:layout_height="wrap_content" android:text="" android:layout_weight="1"/>
        <TextView android:textSize="12sp" android:textColor="#263d60" android:gravity="right" android:id="@+id/detail" android:focusable="true" android:clickable="true" android:layout_width="0dp" android:layout_height="wrap_content" android:text="Click me senpai" android:layout_weight="1"/>
    </LinearLayout>
</LinearLayout>
